<Window x:Class="WPFjuuh.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFjuuh"
        mc:Ignorable="d"
        Title="About" Height="150" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="134*"/>
            <ColumnDefinition Width="9*"/>
            <ColumnDefinition Width="149*"/>
        </Grid.ColumnDefinitions>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Height="61" Margin="66,27,0,0" TextWrapping="Wrap" Text="Programmed by Valtteri Stenholm. All rights reserved 2017. Yee boi" VerticalAlignment="Top" Width="180" Grid.ColumnSpan="3"/>
        <Button x:Name="button" Grid.ColumnSpan="3" Content="Close" HorizontalAlignment="Left" Height="21" Margin="107,88,0,0" VerticalAlignment="Top" Width="77" Click="button_Click"/>

    </Grid>
</Window>
